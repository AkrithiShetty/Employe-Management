<!DOCTYPE html>
<html>

<head>
    <title>Employees</title>
    <link rel='stylesheet' href='/css/style-sheet.css'>
</head>

<body>
    {{>headerEmployee}}
    <table border="2px">
        <thead>
            <tr>
                <th>Full Name</th>
                <th>Email</th>
                <th>Job description</th>
                <th>Phone no</th>

            </tr>
        </thead>
        <tbody>
            {{#each list}}
            <tr t>
                <td>{{this.name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.position}}</td>
                <td>{{this.phone}}</td>

            </tr>
            {{/each}}
        </tbody>
    </table>
    {{>footer}}
</body>

</html>